{
  "model": {
    "hidden_sizes": [64, 32],
    "dropout_rate": 0.3,
    "num_classes": 3
  },
  "training": {
    "learning_rate": 0.001,
    "batch_size": 64,
    "epochs": 60,
    "weight_decay": 0.0001,
    "patience": 10,
    "early_stopping": true,
    "use_augmentation": true,
    "augmentation_prob": 0.5,
    "augmentation_noise_level": 0.05
  },
  "data": {
    "train_ratio": 0.6,
    "val_ratio": 0.2,
    "test_ratio": 0.2,
    "n_features": 15
  },
  "optimization": {
    "optimizer": "Adam",
    "scheduler": "ReduceLROnPlateau",
    "scheduler_factor": 0.5,
    "scheduler_patience": 5
  }
}


